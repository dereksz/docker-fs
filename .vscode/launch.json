{
  "version": "0.2.0",
  "configurations": [
      {
          "name": "Python: Current File",
          "type": "debugpy",
          "request": "launch",
          "program": "${file}",
          "console": "integratedTerminal",
          "cwd": "${fileDirname}",
          "env": {
            "PYTHONPATH": "${workspaceFolder}${pathSeparator}src"
          },
          "justMyCode": false,
      },
      {
          "name": "Python: mount",
          "type": "debugpy",
          "request": "launch",
          "program": "dockerfs.py",
          "console": "integratedTerminal",
          "cwd": "${workspaceFolder}/src",
          "args": [
            ".",
            "/Users/derek.slone-zhen/ow-edge-collection/integration-layer/.dockerfs",
            "unix://Users/derek.slone-zhen/.colima/default/docker.sock",
          ],
          "env": {
            "PYTHONPATH": "${workspaceFolder}${pathSeparator}src"
          },
          "justMyCode": false,
      },
  ]
}
